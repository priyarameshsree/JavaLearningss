import java.util.*;
public class TestPrograms{
	public static void main(String[]args){
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number1");
		int num1=sc.nextInt();
		even(num1);
	}
	public static void even(int number){
		while(number<=0){
			System.out.println("Enter the valid number");
			break;

		}
		if((number>0)&&(number%2==0))
		{
			System.out.println(number+" Is an Even number");
		}
		else if((number>0)&&(number%2!=0))
		{
			System.out.println(number+"Is an odd number");
		}
		}
	}
//Find eligible for vote or not
import java.util.*;
public class TestPrograms{
	public static void main(String[]args){
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the Age");
		int num1=sc.nextInt();
		EligibleforVote(num1);

		}



		public static void EligibleforVote(int age)
		{
			if(age>=18)
			{
				System.out.println("You are eligible for vote");
			}
			else
			{
				System.out.println("You are not eligible for vote");
			}
		}
	}

//Given number is prime number or not
import java.util.*;
public class TestPrograms{
	public static void main(String[]args){
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the Number");
		int num1=sc.nextInt();
		System.out.println(num1+ "is  a primenumber" +" : "+PrimeNumber(num1));
		}

		public static boolean PrimeNumber(int number){
			if(number<=1){
				return false;
			}
			for(int i=2;i<number;i++){
				if(number%i==0){
					return false;
				}
			}
			return true;

		}
	}

//grade based on marks(using while loop and if statement)
import java.util.*;
public class Testprograms2{
	public static void main(String[]args){
		int count=1;
		
		Scanner sc=new Scanner(System.in);
		while(count<=5)
		{
		System.out.println("Enter the  mark");
		 int num1=sc.nextInt();
		count++;
		Grademarks(num1);
		
	}
	
}

	
	public static void Grademarks(int marks)
	{
		if(marks>90)
		{
			System.out.println("Grade A");
		}
		else if(marks>80)
		{
			System.out.println("Grade B");
		}
		else if(marks>70)
		{
			System.out.println("Grade c");		
		}
		else if(marks>60)
		{
			System.out.println("Grade D");
		}
		else if(marks>50)
		{
			System.out.println("Grade E");
		}
		else 
		{
			System.out.println("Poor marks");
		}

		}
	}

	
	




		
	

		
		
