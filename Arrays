//find the product of even numbers present in the array
package inheritance;

import java.util.Arrays;

public class Arra {
	public static void main(String[]args) {
		int array[]= {1,8,7,5,4,3,2,33,88,54,43,23,30,77,99,100};
		int product=1;
		for(int i=0;i<array.length;i++) {
			if(array[i]%2==0) {
				System.out.println(array[i]);
				product=product*array[i];
			}
			
		}
		
		System.out.println("The produt  of even numbers in given array"+product);
}
}

